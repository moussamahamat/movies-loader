pipeline {
    agent none 
    stages {
        stage('Build') { 
            agent {
                docker {
                    image 'python:3.10-alpine' 
                }
            }
            steps {
                sh 'python -m py_compile main.py' 
                stash(name: 'compiled-results', includes: '*.py*') 
            }
        }
    }
}
